1=addition of 2 numbers
public class addtownumbers{

     public static void main(String []args){
         int a=5,b=3,sum;
         sum = a+b;
        System.out.println("addition of tow numbers are  " +sum);
     }
}

2-given numbver is even or odd
public class OddEven{

     public static void main(String []args){
         int num = 9;
         int a;
         a=num%2;
         if(a==0){
         System.out.println("number is even");
             
         }
         else{
             System.out.println("number is odd");
         }
         
     }
}
3=simple intrest
public class simpleIntrest{

     public static void main(String []args){
         int p=100,r=12,i,a,t=1;
         int prt;
         prt = p*r*t;
         i=prt/100;
         a=p+i;
        System.out.println("simple intrest amount is " +i);
        System.out.println("total amount is " +a);
     }
}
4=compoud intrest
public class compoundIntrest{

     public static void main(String []args){
         double p=450,m=1,t=1,a;
         double r=5;
         double rm=r/m;
         double  mt=m*t;
         a=p*(Math.pow((1 + r/ 100), mt));
        System.out.println("Hello World  "+a);
     }
}
